{
  "manifest_version": 2,

  "name": "RDF Viewer",
  "description": "Powerful RDF viewer for Google Chrome.",
  "version": "1.0",

  "background": {
    "page": "background.html"
  },

  "permissions" : [
    "tabs",
    "clipboardWrite",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}